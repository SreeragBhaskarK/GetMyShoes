<% if (locals.noLayout) { %>
    <%- body %>

        <% }else{ %>

            <!DOCTYPE html>
            <html lang="en">

            <head>
                <meta charset="UTF-8">
                <meta http-equiv="X-UA-Compatible" content="IE=edge">
                <meta name="viewport" content="width=device-width, initial-scale=1.0">
                <title>GetMyShoes</title>
                <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css"
                    rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD"
                    crossorigin="anonymous">
                <% if (locals.noShow) { %>

                    <% }else{ %>
                        <% if (locals.admin) { %>
                            <%- include('../partials/admin/admin_Head') %>
                                <% }else{ %>
                                    <!-- head -->
                                    <%- include('../partials/users/user_Head') %>
                                        <!-- /head -->
                                        <% } %>
                                            <% } %>


            </head>

            <body id="top">
                <!-- Page Preloder -->
                <div id="preloder">
                    <div class="loader"></div>
                </div>
                <% if (locals.noShow) { %>

                    <% }else{ %>
                        <% if (locals.admin) { %>
                            <%- include('../partials/admin/admin_Header') %>
                                <% }else{ %>
                                    <%- include('../partials/users/user_Header') %>
                                        <% } %>
                                            <% } %>


                                                <%- body %>
                                                    <% if (locals.noShow) { %>

                                                        <% }else{ %>
                                                            <% if (locals.admin) { %>
                                                                <%- include('../partials/admin/admin_Footer') %>
                                                                    <%- include('../partials/admin/admin_Script') %>
                                                                        <% }else{ %>
                                                                            <!-- footer -->
                                                                            <%- include('../partials/users/user_Footer')
                                                                                %>
                                                                                <!-- /footer -->



                                                                                <!-- script  -->
                                                                                <%- include('../partials/users/user_Script')
                                                                                    %>
                                                                                    <!-- /script -->
                                                                                    <% } %>
                                                                                        <% } %>


                                                                                            <div
                                                                                                class="toast-container position-fixed bottom-0  mb-5 end-0 p-3">
                                                                                                <div id="toasterMessage"
                                                                                                    class="toast align-items-center text-bg-primary border-0"
                                                                                                    role="alert"
                                                                                                    aria-live="assertive"
                                                                                                    aria-atomic="true">
                                                                                                    <div class="d-flex">
                                                                                                        <div style="font-size: 20px;"
                                                                                                            class="toast-body">
                                                                                                            <span
                                                                                                                id="toastMessage"></span>

                                                                                                        </div>
                                                                                                        <button
                                                                                                            type="button"
                                                                                                            class="btn-close btn-close-white me-2 m-auto"
                                                                                                            data-bs-dismiss="toast"
                                                                                                            aria-label="Close"></button>
                                                                                                    </div>
                                                                                                </div>
                                                                                            </div>

                                                                                            <script>
                                                                                                $(window).on('load', function () {
                                                                                                    $(".loader").fadeOut();
                                                                                                    $("#preloder").delay(200).fadeOut("slow");

                                                                                                
                                                                                                });
                                                                                            </script>
            </body>

            </html>


            <% } %>